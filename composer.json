{
    "name": "jurager/documentator",
    "description": "OpenAPI specification generator for Laravel",
    "keywords": ["laravel", "openapi", "swagger", "documentation", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yuri Gerassimov",
            "email": "jurager01@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.25"
    },
    "autoload": {
        "psr-4": {
            "Jurager\\Documentator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jurager\\Documentator\\DocumentatorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}